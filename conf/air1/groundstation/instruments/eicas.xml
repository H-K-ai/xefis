<?xml version='1.0'?>
<xefis-include>
	<layout type='horizontal'>
		<item stretch-factor='1'/>
		<item stretch-factor='18'>
			<layout type='vertical'>
				<item stretch-factor='10'>
					<instrument name='instruments/radial-indicator' instance='engine-L-THR'>
						<settings>
							<setting name='value.modulo' value='10'/>
							<setting name='value.minimum' value='-200'/>
							<setting name='value.maximum' value='800'/>
							<setting name='value.maximum.warning' value='750'/>
							<setting name='value.maximum.critical' value='800'/>
						</settings>
						<properties path='//sensors/engines/1'>
							<property name='value' path='/thrust'/>
							<property name='value.reference' path='/thrust.reference' default='520'/>
							<property name='value.target' path='//input/main/axis/2'/>
						</properties>
					</instrument>
				</item>
				<item stretch-factor='10'>
					<instrument name='instruments/radial-indicator' instance='engine-L-RPM'>
						<settings>
							<setting name='value.modulo' value='10'/>
							<setting name='value.minimum' value='0'/>
							<setting name='value.maximum' value='3500'/>
							<setting name='value.maximum.warning' value='3000'/>
							<setting name='value.maximum.critical' value='3500'/>
						</settings>
						<properties path='//sensors/engines/1'>
							<property name='value' path='/rpm'/>
						</properties>
					</instrument>
				</item>
				<item stretch-factor='10'>
					<instrument name='instruments/radial-indicator' instance='engine-L-TEMP'>
						<settings>
							<setting name='value.minimum' value='0'/>
							<setting name='value.maximum' value='900'/>
							<setting name='value.maximum.warning' value='800'/>
							<setting name='value.maximum.critical' value='900'/>
						</settings>
						<properties path='//sensors/engines/1'>
							<property name='value' path='/temperature'/>
						</properties>
					</instrument>
				</item>
				<item stretch-factor='10'>
					<instrument name='instruments/linear-indicator' instance='engine-L-PITCH'>
						<settings>
							<setting name='value.minimum' value='9.0'/>
							<setting name='value.maximum' value='27.0'/>
							<setting name='value.digits' value='4'/>
							<setting name='value.precision' value='1'/>
							<setting name='style.mirrored' value='false'/>
						</settings>
						<properties path='//sensors/engines/1'>
							<property name='value' path='/pitch'/>
						</properties>
					</instrument>
				</item>
				<item stretch-factor='32'/>
			</layout>
		</item>
		<item stretch-factor='4'>
			<layout type='vertical'>
				<item stretch-factor='9'>
					<instrument name='instruments/label' instance='engine-THR'>
						<label>THR</label>
						<align>hcenter bottom</align>
						<color>#4bf</color>
						<font-size>11</font-size>
					</instrument>
				</item>
				<item stretch-factor='10'>
					<instrument name='instruments/label' instance='engine-RPM'>
						<label>RPM</label>
						<align>hcenter bottom</align>
						<color>#4bf</color>
						<font-size>11</font-size>
					</instrument>
				</item>
				<item stretch-factor='10'>
					<instrument name='instruments/label' instance='engine-TEMP'>
						<label>TEMP</label>
						<align>hcenter bottom</align>
						<color>#4bf</color>
						<font-size>11</font-size>
					</instrument>
				</item>
				<item stretch-factor='1'/>
				<item stretch-factor='10'>
					<instrument name='instruments/label' instance='engine-ESC-PITCH'>
						<label>PTCH</label>
						<align>hcenter vcenter</align>
						<color>#4bf</color>
						<font-size>11</font-size>
					</instrument>
				</item>
				<item stretch-factor='32'/>
			</layout>
		</item>
		<item stretch-factor='18'>
			<layout type='vertical'>
				<item stretch-factor='10'>
					<instrument name='instruments/radial-indicator' instance='engine-R-THR'>
						<settings>
							<setting name='value.modulo' value='10'/>
							<setting name='value.minimum' value='-200'/>
							<setting name='value.maximum' value='800'/>
							<setting name='value.maximum.warning' value='750'/>
							<setting name='value.maximum.critical' value='800'/>
						</settings>
						<properties path='//sensors/engines/2'>
							<property name='value' path='/thrust'/>
							<property name='value.reference' path='/thrust.reference' default='520'/>
							<property name='value.target' path='//input/main/axis/2'/>
						</properties>
					</instrument>
				</item>
				<item stretch-factor='10'>
					<instrument name='instruments/radial-indicator' instance='engine-R-RPM'>
						<settings>
							<setting name='value.modulo' value='10'/>
							<setting name='value.minimum' value='0'/>
							<setting name='value.maximum' value='3500'/>
							<setting name='value.maximum.warning' value='3000'/>
							<setting name='value.maximum.critical' value='3500'/>
						</settings>
						<properties path='//sensors/engines/2'>
							<property name='value' path='/rpm'/>
						</properties>
					</instrument>
				</item>
				<item stretch-factor='10'>
					<instrument name='instruments/radial-indicator' instance='engine-R-TEMP'>
						<settings>
							<setting name='value.minimum' value='0'/>
							<setting name='value.maximum' value='900'/>
							<setting name='value.maximum.warning' value='800'/>
							<setting name='value.maximum.critical' value='900'/>
						</settings>
						<properties path='//sensors/engines/2'>
							<property name='value' path='/temperature'/>
						</properties>
					</instrument>
				</item>
				<item stretch-factor='10'>
					<instrument name='instruments/linear-indicator' instance='engine-R-PITCH'>
						<settings>
							<setting name='value.minimum' value='9.0'/>
							<setting name='value.maximum' value='27.0'/>
							<setting name='value.digits' value='4'/>
							<setting name='value.precision' value='1'/>
							<setting name='style.mirrored' value='true'/>
						</settings>
						<properties path='//sensors/engines/2'>
							<property name='value' path='/pitch'/>
						</properties>
					</instrument>
				</item>
				<item stretch-factor='32'/>
			</layout>
		</item>
		<item stretch-factor='2'/>
		<item stretch-factor='48'>
			<layout type='vertical'>
				<item stretch-factor='50'>
					<instrument name='instruments/eicas'>
						<properties>
							<property name='button.cursor-up' path='/panels/mcp/eicas-up'/>
							<property name='button.cursor-down' path='/panels/mcp/eicas-down'/>
							<property name='button.cursor-del' path='/panels/mcp/eicas-del'/>
							<property name='button.recall' path='/panels/mcp/eicas-recall'/>
							<property name='button.clear' path='/panels/mcp/eicas-clear'/>
						</properties>
						<messages>
							<message path='/systems/adc/altitude/amsl.serviceable' message='ADC ALT AMSL FAIL' fail-on='false'/>
							<message path='/systems/adc/speed/ias.serviceable' message='ADC IAS FAIL' fail-on='false'/>
							<message path='/systems/adc/vertical-speed/serviceable' message='V/SPD FAIL' fail-on='false'/>
							<message path='/systems/flight-director/serviceable' message='FD FAIL' fail-on='false'/>
							<message path='/systems/nc/orientation/serviceable' message='NC ADI FAIL' fail-on='false' severity='critical'/>
							<message path='/sensors/ahrs/serviceable' message='AHRS FAIL' fail-on='false' severity='critical'/>
							<message path='/sensors/pressure/serviceable' message='STATIC PRESS FAIL' fail-on='false'/>
							<message path='/sensors/gps/serviceable' message='GPS FAIL' fail-on='false' severity='critical'/>
							<message path='/sensors/airspeed/serviceable' message='PITOT FAIL' fail-on='false'/>
							<message path='/sensors/radar-altimeter/serviceable' message='RA FAIL' fail-on='false'/>
							<message path='/link/xbee/serviceable' message='LINK FAIL' fail-on='false' severity='critical'/>
						</messages>
					</instrument>
				</item>
				<item stretch-factor='50'>
				</item>
			</layout>
		</item>
	</layout>
</xefis-include>

