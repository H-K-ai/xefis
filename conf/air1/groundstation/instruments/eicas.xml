<?xml version='1.0'?>
<xefis-include>
	<layout type='horizontal'>
		<item stretch-factor='1'/>
		<item stretch-factor='18'>
			<layout type='vertical'>
				<item stretch-factor='10'>
					<module name='instruments/radial-indicator' instance='engine-L-RPM'>
						<settings>
							<setting name='value.modulo' value='10'/>
							<setting name='value.minimum' value='0'/>
							<setting name='value.maximum' value='4000'/>
							<setting name='value.maximum.warning' value='3500'/>
							<setting name='value.maximum.critical' value='3900'/>
						</settings>
						<properties path='//sensors/engines/left'>
							<property name='value' path='/rpm'/>
							<property name='value.normal' path='/rpm.cruise' default='2300'/>
						</properties>
					</module>
				</item>
				<item stretch-factor='10'>
					<module name='instruments/radial-indicator' instance='engine-L-AMP'>
						<settings>
							<setting name='value.precision' value='1'/>
							<setting name='value.minimum' value='0'/>
							<setting name='value.maximum' value='45'/>
							<setting name='value.maximum.warning' value='43'/>
							<setting name='value.maximum.critical' value='45'/>
						</settings>
						<properties path='//sensors/engines/left'>
							<property name='value' path='/amps'/>
						</properties>
					</module>
				</item>
				<item stretch-factor='10'>
					<module name='instruments/radial-indicator' instance='engine-L-TEMP'>
						<settings>
							<setting name='value.minimum' value='0'/>
							<setting name='value.maximum' value='150'/>
							<setting name='value.maximum.warning' value='140'/>
							<setting name='value.maximum.critical' value='150'/>
						</settings>
						<properties path='//sensors/engines/left'>
							<property name='value' path='/temperature'/>
						</properties>
					</module>
				</item>
				<item stretch-factor='10'>
					<module name='instruments/linear-indicator' instance='engine-L-VOLT'>
						<settings>
							<setting name='value.minimum' value='10.0'/>
							<setting name='value.maximum' value='13.0'/>
							<setting name='value.digits' value='4'/>
							<setting name='value.precision' value='1'/>
							<setting name='style.mirrored' value='false'/>
						</settings>
						<properties path='//sensors/engines/left'>
							<property name='value' path='/esc.voltage' default='10.1'/>
						</properties>
					</module>
				</item>
				<item stretch-factor='1'/>
				<item stretch-factor='7'>
					<module name='instruments/linear-indicator' instance='engine-L-ESC-TEMP'>
						<settings>
							<setting name='value.minimum' value='0.0'/>
							<setting name='value.maximum' value='150.0'/>
							<setting name='value.digits' value='3'/>
							<setting name='value.precision' value='0'/>
							<setting name='style.mirrored' value='false'/>
						</settings>
						<properties path='//sensors/engines/left'>
							<property name='value' path='/esc.temperature' default='74'/>
						</properties>
					</module>
				</item>
				<item stretch-factor='17'/>
			</layout>
		</item>
		<item stretch-factor='4'>
			<layout type='vertical'>
				<item stretch-factor='9'>
					<module name='instruments/label' instance='engine-RPM'>
						<label>RPM</label>
						<align>hcenter bottom</align>
						<color>#4bf</color>
						<font-size>11</font-size>
					</module>
				</item>
				<item stretch-factor='10'>
					<module name='instruments/label' instance='engine-AMP'>
						<label>AMP</label>
						<align>hcenter bottom</align>
						<color>#4bf</color>
						<font-size>11</font-size>
					</module>
				</item>
				<item stretch-factor='10'>
					<module name='instruments/label' instance='engine-TEMP'>
						<label>TEMP</label>
						<align>hcenter bottom</align>
						<color>#4bf</color>
						<font-size>11</font-size>
					</module>
				</item>
				<item stretch-factor='1'/>
				<item stretch-factor='10'>
					<module name='instruments/label' instance='engine-ESC-VOLT'>
						<label>VOLT</label>
						<align>hcenter vcenter</align>
						<color>#4bf</color>
						<font-size>11</font-size>
					</module>
				</item>
				<item stretch-factor='1'/>
				<item stretch-factor='7'>
					<module name='instruments/label' instance='engine-ESC-TEMP'>
						<label>ESCT</label>
						<align>hcenter vcenter</align>
						<color>#4bf</color>
						<font-size>11</font-size>
					</module>
				</item>
				<item stretch-factor='17'/>
			</layout>
		</item>
		<item stretch-factor='18'>
			<layout type='vertical'>
				<item stretch-factor='10'>
					<module name='instruments/radial-indicator' instance='engine-R-RPM'>
						<settings>
							<setting name='value.modulo' value='10'/>
							<setting name='value.minimum' value='0'/>
							<setting name='value.maximum' value='4000'/>
							<setting name='value.maximum.warning' value='3500'/>
							<setting name='value.maximum.critical' value='3900'/>
						</settings>
						<properties path='//sensors/engines/right'>
							<property name='value' path='/rpm'/>
							<property name='value.normal' path='/rpm.cruise' default='2300'/>
						</properties>
					</module>
				</item>
				<item stretch-factor='10'>
					<module name='instruments/radial-indicator' instance='engine-R-AMP'>
						<settings>
							<setting name='value.precision' value='1'/>
							<setting name='value.minimum' value='0'/>
							<setting name='value.maximum' value='45'/>
							<setting name='value.maximum.warning' value='43'/>
							<setting name='value.maximum.critical' value='45'/>
						</settings>
						<properties path='//sensors/engines/right'>
							<property name='value' path='/amps'/>
						</properties>
					</module>
				</item>
				<item stretch-factor='10'>
					<module name='instruments/radial-indicator' instance='engine-R-TEMP'>
						<settings>
							<setting name='value.minimum' value='0'/>
							<setting name='value.maximum' value='150'/>
							<setting name='value.maximum.warning' value='140'/>
							<setting name='value.maximum.critical' value='150'/>
						</settings>
						<properties path='//sensors/engines/right'>
							<property name='value' path='/temperature'/>
						</properties>
					</module>
				</item>
				<item stretch-factor='10'>
					<module name='instruments/linear-indicator' instance='engine-R-VOLT'>
						<settings>
							<setting name='value.minimum' value='10.0'/>
							<setting name='value.maximum' value='13.0'/>
							<setting name='value.digits' value='4'/>
							<setting name='value.precision' value='1'/>
							<setting name='style.mirrored' value='true'/>
						</settings>
						<properties path='//sensors/engines/right'>
							<property name='value' path='/esc.voltage' default='9.8'/>
						</properties>
					</module>
				</item>
				<item stretch-factor='1'/>
				<item stretch-factor='7'>
					<module name='instruments/linear-indicator' instance='engine-R-ESC-TEMP'>
						<settings>
							<setting name='value.minimum' value='0.0'/>
							<setting name='value.maximum' value='150.0'/>
							<setting name='value.digits' value='3'/>
							<setting name='value.precision' value='0'/>
							<setting name='style.mirrored' value='true'/>
						</settings>
						<properties path='//sensors/engines/right'>
							<property name='value' path='/esc.temperature' default='72'/>
						</properties>
					</module>
				</item>
				<item stretch-factor='17'/>
			</layout>
		</item>
		<item stretch-factor='50'>
		</item>
	</layout>
</xefis-include>

