										<row>
											<label>SYS BEC 2</label>
											<value path='/' format='%.1f V' nil='--  '/>
										</row>
										<row>
											<label>FCTL BEC 1</label>
											<value path='/' format='%.1f V' nil='--  '/>
										</row>
										<row>
											<label>FCTL BEC 2</label>
											<value path='/' format='%.1f V' nil='--  '/>
										</row>
									</table>
								</instrument>
							</item>
						</layout>
					<!--/group-->
				</item>
				<item stretch-factor='50'/>
			</layout>
		</item>
	</layout>
</xefis-include>

