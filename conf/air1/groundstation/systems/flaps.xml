<?xml version='1.0'?>
<xefis-include>
	<module name='systems/flaps-control'>
		<settings>
			<setting name='settings-list' value='0,5,10,20,30'/>
		</settings>
		<properties>
			<!-- Input -->
			<property name='input.up' path='/controls/flaps-up'/>
			<property name='input.down' path='/controls/flaps-down'/>
			<property name='input.setting' path='/settings/flaps/setting'/>
		</properties>
	</module>
	<module name='systems/flaps-bugs'>
		<properties>
			<property name='input.flaps-setting' path='/settings/flaps/setting'/>
			<property name='output.flaps-bug.speed' path='/systems/flaps/bugs/speed'/>
			<property name='output.flaps-bug.label' path='/systems/flaps/bugs/label'/>
		</properties>
	</module>
</xefis-include>

