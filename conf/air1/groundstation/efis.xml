									</directory>
									<directory path='/systems/fdc'>
										<property name='speed.gs' path='/speed/ground-speed'/>
										<property name='speed.tas' path='/speed/true-airspeed'/>
										<property name='track.lateral-magnetic' path='/track/lateral-magnetic'/>
										<property name='track.delta.lateral' path='/track/lateral-delta'/>
										<property name='orientation.heading.magnetic' path='/orientation/magnetic-heading'/>
										<property name='orientation.heading.true' path='/orientation/true-heading'/>
										<property name='position.source' path='/position/source'/>
										<property name='position.latitude' path='/position/latitude'/>
										<property name='position.longitude' path='/position/longitude'/>
										<property name='wind.from-mag-heading' path='/wind/magnetic-orientation-from'/>
										<property name='wind.tas' path='/wind/true-airspeed'/>
										<property name='localizer-id' path='/localizer/identifier'/>
										<property name='climb-glide-ratio' path='/performance/climb-glide-ratio'/>
									</directory>
									<directory path='/systems/rcms'>
										<property name='home.distance.vlos' path='/home/vlos-distance'/>
										<property name='home.distance.ground' path='/home/ground-distance'/>
										<property name='home.distance.vertical' path='/home/vertical-distance'/>
										<property name='home.true-direction' path='/home/true-direction'/>
									</directory>
								</properties>
							</module>
						</item>
					</layout>
				</item>
				<separator/>
