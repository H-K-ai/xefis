					<layout type='stack' path='//efis/mfd-mode'>
						<item>
							<layout type='horizontal'>
								<item stretch-factor='1'/>
								<item stretch-factor='18'>
									<layout type='vertical'>
										<item stretch-factor='10'>
											<module name='instruments/radial-indicator' instance='engine-L-RPM'>
												<settings>
													<setting name='value.modulo' value='10'/>
													<setting name='value.minimum' value='0'/>
													<setting name='value.maximum' value='4000'/>
													<setting name='value.maximum.warning' value='3500'/>
													<setting name='value.maximum.critical' value='3900'/>
												</settings>
												<properties path='//sensors/engines/left'>
													<property name='value' path='/rpm'/>
													<property name='value.normal' path='/rpm.cruise' default='2300'/>
												</properties>
											</module>
										</item>
										<item stretch-factor='10'>
											<module name='instruments/radial-indicator' instance='engine-L-AMP'>
