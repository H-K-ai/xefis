<?xml version='1.0'?>
<xefis-include>
	<windows>
		<window width='1366' height='768' full-screen='false'>
			<layout type='horizontal'>
				<item stretch-factor='4'>
					<layout type='vertical'>
						<item stretch-factor='8'>
							<module name='instruments/efis'>
								<settings>
									<setting name='speed-ladder.line-every' value='10'/>
									<setting name='speed-ladder.number-every' value='20'/>
									<setting name='speed-ladder.extent' value='124'/>
									<setting name='speed-ladder.minimum' value='20'/>
									<setting name='speed-ladder.maximum' value='900'/>
									<setting name='altitude-ladder.line-every' value='100'/>
									<setting name='altitude-ladder.number-every' value='200'/>
									<setting name='altitude-ladder.bold-every' value='500'/>
									<setting name='altitude-ladder.extent' value='825'/>
								</settings>
								<properties path='/instruments/efis'>
									<directory path='/settings'>
										<property name='style.old' path='/old-style' default='false'/>
										<property name='fpv.visible' path='/fpv.visible' default='true'/>
									</directory>
									<directory path='//systems/adc/speed'>
										<property name='speed.ias.serviceable' path='/ias.serviceable'/>
										<property name='speed.ias' path='/ias'/>
										<property name='speed.ias.lookahead' path='/ias.lookahead'/>
										<property name='speed.mach' path='/mach'/>
									</directory>
									<directory path='//systems/nc/orientation'>
										<property name='orientation.serviceable' path='/serviceable'/>
										<property name='orientation.pitch' path='/pitch'/>
										<property name='orientation.roll' path='/roll'/>
										<property name='orientation.heading.magnetic' path='/heading.magnetic'/>
										<property name='orientation.heading.true' path='/heading.true'/>
										<property name='orientation.heading.numbers-visible' path='/heading.numbers-visible'/>
									</directory>
									<directory path='//systems/nc/track'>
										<property name='track.lateral.magnetic' path='/lateral.magnetic'/>
										<property name='track.lateral.true' path='/lateral.true'/>
										<property name='track.vertical' path='/vertical'/>
									</directory>
									<directory path='//sensors/slip-skid'>
										<property name='slip-skid' path='/slip-skid'/>
									</directory>
									<directory path='//systems/adc/altitude/amsl'>
										<property name='altitude.amsl.serviceable' path='/serviceable'/>
										<property name='altitude.amsl' path='/altitude'/>
										<property name='altitude.amsl.lookahead' path='/altitude.lookahead'/>
									</directory>
									<property name='altitude.agl' path='//sensors/radar-altimeter/agl'/>
									<directory path='//systems/adc/vertical-speed'>
										<property name='vertical-speed.serviceable' path='/serviceable'/>
										<property name='vertical-speed' path='/vertical-speed'/>
										<property name='vertical-speed.variometer' path='/variometer'/>
									</directory>
									<directory path='//settings/altimeter'>
										<property name='pressure.qnh' path='/pressure.qnh'/>
										<property name='pressure.use-std' path='/use-std-pressure'/>
										<property name='pressure.display-hpa' path='/pressure-in-hpa'/>
									</directory>
								</properties>
							</module>
						</item>
						<item stretch-factor='5'>
							<module name='instruments/hsi' instance='aux'>
								<properties>
									<directory path='/settings/hsi'>
										<property name='track.visible' path='/follow-track'/>
										<property name='orientation.display-true-heading' path='/use-true-heading'/>
									</directory>
									<directory path='/settings/hsi-aux'>
										<property name='display-mode' path='/mode'/>
										<property name='range' path='/range'/>
										<property name='trend-vector-range' path='/trend-vector-range'/>
									</directory>
									<directory path='/settings/flight-director'>
										<property name='cmd.visible' path='/visible'/>
										<property name='cmd.track-visible' path='/enabled'/>
										<property name='cmd.heading' path='/cmd-magnetic-heading-and-track'/>
									</directory>
									<property name='position.source' path='//systems/nc/position/source'/>
									<property name='position.latitude' path='//systems/nc/position/latitude'/>
									<property name='position.longitude' path='//systems/nc/position/longitude'/>
									<property name='orientation.heading.magnetic' path='//systems/nc/orientation/heading.magnetic'/>
									<property name='orientation.heading.true' path='//systems/nc/orientation/heading.true'/>
									<property name='speed.gs' path='//systems/nc/track/ground-speed'/>
									<property name='speed.tas' path='//systems/adc/speed/tas'/>
									<property name='track.lateral-magnetic' path='//systems/nc/track/lateral.magnetic'/>
									<property name='track.delta.lateral' path='//systems/nc/track/lateral.delta'/>
									<directory path='/systems/pc'>
										<property name='wind.from-mag-heading' path='/wind/magnetic-orientation-from'/>
										<property name='wind.tas' path='/wind/true-airspeed'/>
										<property name='localizer-id' path='/localizer/identifier'/>
										<property name='climb-glide-ratio' path='/performance/climb-glide-ratio'/>
										<property name='altitude.target.reach-distance' path='/performance/target-altitude-reach-distance'/>
									</directory>
									<directory path='/systems/rcms'>
										<property name='home.distance.vlos' path='/home/vlos-distance'/>
										<property name='home.distance.ground' path='/home/ground-distance'/>
										<property name='home.distance.vertical' path='/home/vertical-distance'/>
										<property name='home.true-direction' path='/home/true-direction'/>
									</directory>
								</properties>
							</module>
						</item>
					</layout>
				</item>
				<separator/>
				<item stretch-factor='4'>
					<layout type='vertical'>
						<item stretch-factor='6'>
							<module name='instruments/hsi' instance='main'>
								<properties>
									<directory path='/settings/hsi'>
										<property name='track.visible' path='/follow-track'/>
										<property name='orientation.display-true-heading' path='/use-true-heading'/>
									</directory>
									<directory path='/settings/hsi-nav'>
										<property name='display-mode' path='/mode'/>
										<property name='range' path='/range'/>
										<property name='trend-vector-range' path='/trend-vector-range'/>
									</directory>
									<directory path='/settings/flight-director'>
										<property name='cmd.visible' path='/visible'/>
										<property name='cmd.track-visible' path='/enabled'/>
										<property name='cmd.heading' path='/cmd-magnetic-heading-and-track'/>
									</directory>
									<property name='position.source' path='//systems/nc/position/source'/>
									<property name='position.latitude' path='//systems/nc/position/latitude'/>
									<property name='position.longitude' path='//systems/nc/position/longitude'/>
									<property name='orientation.heading.magnetic' path='//systems/nc/orientation/heading.magnetic'/>
									<property name='orientation.heading.true' path='//systems/nc/orientation/heading.true'/>
									<property name='speed.gs' path='//systems/nc/track/ground-speed'/>
									<property name='speed.tas' path='//systems/adc/speed/tas'/>
									<property name='track.lateral-magnetic' path='//systems/nc/track/lateral.magnetic'/>
									<property name='track.delta.lateral' path='//systems/nc/track/lateral.delta'/>
									<directory path='/systems/pc'>
										<property name='wind.from-mag-heading' path='/wind/magnetic-orientation-from'/>
										<property name='wind.tas' path='/wind/true-airspeed'/>
										<property name='localizer-id' path='/localizer/identifier'/>
										<property name='climb-glide-ratio' path='/performance/climb-glide-ratio'/>
										<property name='altitude.target.reach-distance' path='/performance/target-altitude-reach-distance'/>
									</directory>
								</properties>
							</module>
						</item>
						<separator/>
						<item stretch-factor='1'>
							<layout type='horizontal'>
								<item stretch-factor='1'>
								</item>
								<item stretch-factor='15'>
									<layout type='horizontal'>
										<item>
											<module name='instruments/radial-indicator' instance='N1'>
												<properties path='/sensors/engine'>
													<property name='value' path='/n1'/>
													<property name='target' path='/config/n1-target' default='18.7'/>
													<property name='value-minimum' path='/config/n1-minimum' default='0.0'/>
													<property name='value-warning' path='/config/n1-warning' default='107.0'/>
													<property name='value-critical' path='/config/n1-critical' default='110.0'/>
													<property name='value-maximum' path='/config/n1-maximum' default='110.0'/>
												</properties>
											</module>
										</item>
										<item>
											<module name='instruments/radial-indicator' instance='N2'>
												<properties path='/sensors/engine'>
													<property name='value' path='/n2'/>
													<property name='value-minimum' path='/config/n2-minimum' default='0.0'/>
													<property name='value-warning' path='/config/n2-warning' default='107.0'/>
													<property name='value-critical' path='/config/n2-critical' default='110.0'/>
													<property name='value-maximum' path='/config/n2-maximum' default='110.0'/>
												</properties>
											</module>
										</item>
										<item>
											<module name='instruments/radial-indicator' instance='EGT'>
												<properties path='/sensors/engine'>
													<property name='value' path='/egt'/>
													<property name='value-minimum' path='/config/egt-minimum' default='0.0'/>
													<property name='value-maximum' path='/config/egt-maximum' default='850.0'/>
													<property name='value-warning' path='/config/egt-warning' default='800.0'/>
													<property name='value-critical' path='/config/egt-critical' default='850.0'/>
													<property name='value-normal' path='/config/egt-normal' default='715.0'/>
												</properties>
											</module>
										</item>
										<item>
											<module name='instruments/radial-indicator' instance='EPR'>
												<properties path='/sensors/engine'>
													<property name='value' path='/epr'/>
													<property name='value-minimum' path='/config/epr-minimum' default='1.0'/>
													<property name='value-maximum' path='/config/epr-maximum' default='4.0'/>
													<property name='value-normal' path='/config/epr-normal' default='3.0'/>
												</properties>
											</module>
										</item>
									</layout>
								</item>
								<item stretch-factor='1'>
								</item>
							</layout>
						</item>
					</layout>
				</item>
			</layout>
		</window>
	</windows>
</xefis-include>

