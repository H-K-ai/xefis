<?xml version='1.0'?>
<xefis-include>
	<windows>
		<window width='1366' height='768' full-screen='false'>
			<layout type='horizontal'>
				<item stretch-factor='4'>
					<layout type='vertical'>
						<item stretch-factor='8'>
							<module name='instruments/efis'>
								<properties>
									<directory path='/instruments/efis'>
										<property name='speed-ladder-line-every' path='/speed-ladder/line-every' default='10'/>
										<property name='speed-ladder-number-every' path='/speed-ladder/number-every' default='20'/>
										<property name='speed-ladder-extent' path='/speed-ladder/extent' default='124'/>
										<property name='speed-ladder-minimum' path='/speed-ladder/minimum' default='20'/>
										<property name='speed-ladder-maximum' path='/speed-ladder/maximum' default='900'/>
										<property name='altitude-ladder-line-every' path='/altitude-ladder/line-every' default='100'/>
										<property name='altitude-ladder-number-every' path='/altitude-ladder/number-every' default='200'/>
										<property name='altitude-ladder-bold-every' path='/altitude-ladder/bold-every' default='500'/>
										<property name='altitude-ladder-extent' path='/altitude-ladder/extent' default='825'/>
										<property name='heading-numbers-visible' path='/adi/heading-numbers-visible' default='false'/>
										<property name='orientation-roll-limit' path='/orientation/roll-warning' default='35 deg'/>
										<property name='slip-skid-limit' path='/slip-skid/slip-skid-warning' default='1.5'/>
										<property name='flight-path-marker-visible' path='/flight-path-marker/visible' default='true'/>
										<property name='minimums-altitude' path='/minimums-altitude' default='3000 ft'/>
										<property name='pressure-qnh' path='//systems/fdc/settings/pressure/qnh'/>
										<property name='pressure-display-hpa' path='//systems/fdc/settings/pressure-display-hpa'/>
										<property name='use-standard-pressure' path='//systems/fdc/settings/use-standard-pressure'/>
										<property name='commanded-settings-visible' path='/command/visible' default='true'/>
										<property name='fma-visible' path='/fma/visible' default='true'/>
										<property name='flight-director-visible' path='/flight-director/visible' default='false'/>
										<property name='cmd-settings-visible' path='/flight-director/cmd-settings-visible' default='true'/>
										<property name='control-stick-visible' path='/control-stick/visible' default='true'/>
										<property name='approach-reference-visible' path='/approach/visible' default='true'/>
										<property name='control-hint-visible' path='/control/hint-visible' default='false'/>
										<property name='control-hint' path='/control/hint' default=''/>
										<property name='novspd-flag' path='/novspd-flag' default='false'/>
									</directory>
									<directory path='/systems/fdc'>
										<property name='ias-minimum' path='/speed/ias-minimum'/>
										<property name='ias-warning' path='/speed/ias-minimum-maneuver'/>
										<property name='ias-maximum' path='/speed/ias-maximum'/>
										<property name='ias-lookahead' path='/speed/ias-lookahead'/>
										<property name='mach' path='/speed/mach'/>
										<property name='orientation-true-heading' path='/orientation/true-heading'/>
										<property name='flight-path-marker-alpha' path='/fpm/alpha'/>
										<property name='flight-path-marker-beta' path='/fpm/beta'/>
										<property name='magnetic-track' path='/flight-path/magnetic-track'/>
										<property name='altitude' path='/pressure-altitude/amsl'/>
										<property name='altitude-lookahead' path='/pressure-altitude/amsl-lookahead'/>
										<property name='cbr' path='/pressure-altitude/climb-rate'/>
										<property name='vertical-deviation' path='/localizer/vertical-deviation'/>
										<property name='lateral-deviation' path='/localizer/lateral-deviation'/>
										<property name='dme-distance' path='/localizer/distance'/>
										<property name='localizer-id' path='/localizer/identifier'/>
										<property name='approach-type-hint' path='/localizer/type'/>
										<property name='orientation-pitch-limit' path='/aoa/pitch-limit'/>
									</directory>
									<directory path='/sensors'>
										<property name='ias' path='/speed/ias'/>
										<property name='orientation-pitch' path='/imu/pitch'/>
										<property name='orientation-roll' path='/imu/roll'/>
										<property name='orientation-magnetic-heading' path='/compass/magnetic-heading'/>
										<property name='slip-skid' path='/slip-skid/slip-skid'/>
										<property name='altitude-agl' path='/radar-altimeter/agl'/>
									</directory>
									<directory path='/systems/flight-director/settings'>
										<property name='cmd-setting-ias' path='//systems/autothrottle/settings/ias' default='100 kt'/>
										<property name='cmd-setting-altitude' path='/altitude' default='999 ft'/>
										<property name='cmd-setting-cbr' path='/vertical-speed' default='300 fpm'/>
									</directory>
									<directory path='/systems/flight-director'>
										<property name='fma-speed-hint' path='/hints/speed-hint' default='TEST'/>
										<property name='fma-speed-small-hint' path='/hints/speed-small-hint' default='TEST'/>
										<property name='fma-lateral-hint' path='/hints/lateral-hint'/>
										<property name='fma-lateral-small-hint' path='/hints/lateral-small-hint'/>
										<property name='fma-vertical-hint' path='/hints/vertical-hint'/>
										<property name='fma-vertical-small-hint' path='/hints/vertical-small-hint'/>
										<property name='flight-director-pitch' path='/pitch'/>
										<property name='flight-director-roll' path='/roll'/>
									</directory>
									<directory path='/systems/fly-by-wire'>
										<property name='control-stick-pitch' path='/control-stick/pitch'/>
										<property name='control-stick-roll' path='/control-stick/roll'/>
									</directory>
								</properties>
							</module>
						</item>
						<item stretch-factor='5'>
							<module name='instruments/hsi'>
								<properties>
									<directory path='/instruments/hsi-aux'>
										<property name='display-mode' path='/display-mode' default='2'/>
										<property name='range' path='/range' default='2 nm'/>
										<property name='trend-vector-range' path='/trend-vector-range' default='2 nm'/>
										<property name='cmd-settings-visible' path='/cmd-settings-visible' default='true'/>
									</directory>
									<directory path='/systems/fdc'>
										<property name='gs' path='/speed/ground-speed'/>
										<property name='tas' path='/speed/true-airspeed'/>
										<property name='magnetic-track' path='/track/magnetic-heading'/>
										<property name='orientation-magnetic-heading' path='/orientation/magnetic-heading'/>
										<property name='orientation-true-heading' path='/orientation/true-heading'/>
										<property name='position-source' path='/position/source'/>
										<property name='position-latitude' path='//sensors/gps/latitude'/>
										<property name='position-longitude' path='//sensors/gps/longitude'/>
										<property name='wind-from-mag-heading' path='/wind/magnetic-orientation-from'/>
										<property name='wind-tas' path='/wind/true-airspeed'/>
										<property name='localizer-id' path='/localizer/identifier'/>
										<property name='use-true-heading' path='/settings/use-true-heading'/>
										<property name='display-track' path='/settings/follow-track'/>
										<property name='climb-glide-ratio' path='/performance/climb-glide-ratio'/>
									</directory>
									<directory path='/systems/flight-director'>
										<property name='cmd-track-visible' path='/settings/enabled'/>
										<property name='cmd-setting-heading' path='/settings/magnetic-heading-and-track'/>
									</directory>
									<directory path='/systems/rcms'>
										<property name='home.distance.vlos' path='/home/vlos-distance'/>
										<property name='home.distance.ground' path='/home/ground-distance'/>
										<property name='home.true-direction' path='/home/true-direction'/>
									</directory>
								</properties>
							</module>
						</item>
					</layout>
				</item>
				<separator/>
				<item stretch-factor='4'>
					<layout type='vertical'>
						<item stretch-factor='6'>
							<module name='instruments/hsi'>
								<properties>
									<directory path='/instruments/hsi-nav'>
										<property name='display-mode' path='/display-mode' default='0'/>
										<property name='range' path='/range' default='20 nm'/>
										<property name='trend-vector-range' path='/trend-vector-range' default='2 nm'/>
										<property name='cmd-settings-visible' path='/cmd-settings-visible' default='true'/>
									</directory>
									<directory path='/systems/fdc'>
										<property name='gs' path='/speed/ground-speed'/>
										<property name='tas' path='/speed/true-airspeed'/>
										<property name='magnetic-track' path='/track/magnetic-heading'/>
										<property name='orientation-magnetic-heading' path='/orientation/magnetic-heading'/>
										<property name='orientation-true-heading' path='/orientation/true-heading'/>
										<property name='position-source' path='/position/source'/>
										<property name='position-latitude' path='//sensors/gps/latitude'/>
										<property name='position-longitude' path='//sensors/gps/longitude'/>
										<property name='wind-from-mag-heading' path='/wind/magnetic-orientation-from'/>
										<property name='wind-tas' path='/wind/true-airspeed'/>
										<property name='localizer-id' path='/localizer/identifier'/>
										<property name='use-true-heading' path='/settings/use-true-heading'/>
										<property name='display-track' path='/settings/follow-track'/>
										<property name='climb-glide-ratio' path='/performance/climb-glide-ratio'/>
									</directory>
									<directory path='/systems/flight-director'>
										<property name='cmd-track-visible' path='/settings/enabled'/>
										<property name='cmd-setting-heading' path='/settings/magnetic-heading-and-track'/>
									</directory>
								</properties>
							</module>
						</item>
						<separator/>
						<item stretch-factor='1'>
							<layout type='horizontal'>
								<item stretch-factor='1'>
								</item>
								<item stretch-factor='15'>
									<layout type='horizontal'>
										<item>
											<module name='instruments/radial-indicator'>
												<properties path='/sensors/engine'>
													<property name='value' path='/n1'/>
													<property name='target' path='/config/n1-target' default='18.7'/>
													<property name='value-minimum' path='/config/n1-minimum' default='0.0'/>
													<property name='value-warning' path='/config/n1-warning' default='107.0'/>
													<property name='value-critical' path='/config/n1-critical' default='110.0'/>
													<property name='value-maximum' path='/config/n1-maximum' default='110.0'/>
												</properties>
											</module>
										</item>
										<item>
											<module name='instruments/radial-indicator'>
												<properties path='/sensors/engine'>
													<property name='value' path='/n2'/>
													<property name='value-minimum' path='/config/n2-minimum' default='0.0'/>
													<property name='value-warning' path='/config/n2-warning' default='107.0'/>
													<property name='value-critical' path='/config/n2-critical' default='110.0'/>
													<property name='value-maximum' path='/config/n2-maximum' default='110.0'/>
												</properties>
											</module>
										</item>
										<item>
											<module name='instruments/radial-indicator'>
												<properties path='/sensors/engine'>
													<property name='value' path='/egt'/>
													<property name='value-minimum' path='/config/egt-minimum' default='0.0'/>
													<property name='value-maximum' path='/config/egt-maximum' default='850.0'/>
													<property name='value-warning' path='/config/egt-warning' default='800.0'/>
													<property name='value-critical' path='/config/egt-critical' default='850.0'/>
													<property name='value-normal' path='/config/egt-normal' default='715.0'/>
												</properties>
											</module>
										</item>
										<item>
											<module name='instruments/radial-indicator'>
												<properties path='/sensors/engine'>
													<property name='value' path='/epr'/>
													<property name='value-minimum' path='/config/epr-minimum' default='1.0'/>
													<property name='value-maximum' path='/config/epr-maximum' default='4.0'/>
													<property name='value-normal' path='/config/epr-normal' default='3.0'/>
												</properties>
											</module>
										</item>
									</layout>
								</item>
								<item stretch-factor='1'>
								</item>
							</layout>
						</item>
					</layout>
				</item>
			</layout>
		</window>
	</windows>
</xefis-include>

