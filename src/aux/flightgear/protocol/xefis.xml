<?xml version="1.0"?>
<PropertyList>
	<generic>
		<comment>
			Copy this file to the 'Protocol' directory of the FlightGear (usually /usr/share/flightgear/data/Protocol/).
			Run FlightGear with '--generic=socket,out,24,127.0.0.1,9000,udp,xefis' on command line.
		</comment>
		<output>
			<line_separator>newline</line_separator>
			<var_separator>,</var_separator>
			<chunk>
				<name>ias</name>
				<type>float</type>
				<format>ias=%f</format>
				<node>/instrumentation/airspeed-indicator/indicated-speed-kt</node>
			</chunk>
			<chunk>
				<name>groundspeed</name>
				<type>float</type>
				<format>gs=%f</format>
				<node>/velocities/groundspeed-kt</node>
			</chunk>
			<chunk>
				<name>tas</name>
				<type>float</type>
				<format>tas=%f</format>
				<node>/instrumentation/airspeed-indicator/true-speed-kt</node>
			</chunk>
			<chunk>
				<name>mach</name>
				<type>float</type>
				<format>mach=%f</format>
				<node>/velocities/mach</node>
			</chunk>
			<chunk>
				<name>heading</name>
				<type>float</type>
				<format>heading=%f</format>
				<node>/orientation/heading-deg</node>
			</chunk>
			<chunk>
				<name>heading-mag</name>
				<type>float</type>
				<format>heading-mag=%f</format>
				<node>/orientation/heading-magnetic-deg</node>
			</chunk>
			<chunk>
				<name>altitude</name>
				<type>float</type>
				<format>altitude=%f</format>
				<node>/instrumentation/altimeter/indicated-altitude-ft</node>
			</chunk>
			<chunk>
				<name>altimeter-inhg</name>
				<type>float</type>
				<format>altimeter-inhg=%f</format>
				<node>/instrumentation/altimeter/setting-inhg</node>
			</chunk>
			<chunk>
				<name>cbr-fpm</name>
				<type>float</type>
				<format>cbr=%f</format>
				<node>/instrumentation/vertical-speed-indicator/indicated-speed-fpm</node>
			</chunk>
			<chunk>
				<name>cbr-2-fps</name>
				<type>float</type>
				<format>cbr-2=%f</format>
				<node>/velocities/vertical-speed-fps</node>
			</chunk>
			<chunk>
				<name>pitch</name>
				<type>float</type>
				<format>pitch=%f</format>
				<node>/instrumentation/attitude-indicator/indicated-pitch-deg</node>
			</chunk>
			<chunk>
				<name>roll</name>
				<type>float</type>
				<format>roll=%f</format>
				<node>/instrumentation/attitude-indicator/indicated-roll-deg</node>
			</chunk>
			<chunk>
				<name>latitude</name>
				<type>float</type>
				<format>latitude=%.10f</format>
				<node>/position/latitude-deg</node>
			</chunk>
			<chunk>
				<name>longitude</name>
				<type>float</type>
				<format>longitude=%.10f</format>
				<node>/position/longitude-deg</node>
			</chunk>
			<chunk>
				<name>time</name>
				<format>time=%d</format>
				<node>/instrumentation/clock/indicated-sec</node>
			</chunk>
			<chunk>
				<name>turn-rate</name>
				<type>float</type>
				<format>turn-rate=%f</format>
				<node>/instrumentation/turn-indicator/indicated-turn-rate</node>
			</chunk>
			<chunk>
				<name>slip-skid</name>
				<type>float</type>
				<format>slip-skid=%f</format>
				<node>/instrumentation/slip-skid-ball/indicated-slip-skid</node>
			</chunk>
		</output>
	</generic>
</PropertyList>
