<?xml version='1.0'?>
<xefis-include>
	<modules>
		<module name='io/bmp085' disabled='true'>
			<properties>
				<directory path='/sensors'>
					<property name='pressure' path='/pressure/pressure'/>
					<property name='pressure.read-interval' path='/pressure/read-interval' default='500 ms'/>
					<property name='temperature' path='/temperature/outside.k'/>
					<property name='temperature.read-interval' path='/temperature/read-interval' default='30 ms'/>
				</directory>
			</properties>
		</module>
	</modules>
</xefis-include>

