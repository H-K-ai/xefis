<?xml version="1.0"?>
<PropertyList>
	<generic>
		<comment>
			Copy this file to the 'Protocol' directory of the FlightGear (usually /usr/share/flightgear/data/Protocol/).
			Run FlightGear with '--generic=socket,bi,40,127.0.0.1,9000,udp,xefis' on command line.
			Configure the "io/flightgear" module in the Xefis configuration files. Use the same UDP port addresses.
		</comment>
		<!--input>
			<line_separator>newline</line_separator>
			<var_separator>,</var_separator>

			<chunk>
				<name>a</name>
				<node>/controls/flight/aileron</node>
				<type>double</type>
				<format>%f</format>
			</chunk>
			<chunk>
				<name>e</name>
				<node>/controls/flight/elevator</node>
				<type>double</type>
				<format>%f</format>
			</chunk>
			<chunk>
				<name>r</name>
				<node>/controls/flight/rudder</node>
				<type>double</type>
				<format>%f</format>
			</chunk>
			<chunk>
				<name>at</name>
				<node>/controls/flight/aileron-trim</node>
				<type>double</type>
				<format>%f</format>
			</chunk>
			<chunk>
				<name>et</name>
				<node>/controls/flight/elevator-trim</node>
				<type>double</type>
				<format>%f</format>
			</chunk>
			<chunk>
				<name>rt</name>
				<node>/controls/flight/rudder-trim</node>
				<type>double</type>
				<format>%f</format>
			</chunk>
			<chunk>
				<name>f</name>
				<node>/controls/flight/flaps</node>
				<type>double</type>
				<format>%f</format>
			</chunk>
			<chunk>
				<name>s</name>
				<node>/controls/flight/slats</node>
				<type>double</type>
				<format>%f</format>
			</chunk>
			<chunk>
				<name>sb</name>
				<node>/controls/flight/speedbrake</node>
				<type>double</type>
				<format>%f</format>
			</chunk>
			<chunk>
				<name>sp</name>
				<node>/controls/flight/spoilers</node>
				<type>double</type>
				<format>%f</format>
			</chunk>
		</input-->
		<output>
			<binary_mode>true</binary_mode>
			<byte_order>host</byte_order>

			<!-- Autopilot/autothrottle -->
			<chunk>
				<name>apa</name>
				<type>double</type>
				<node>/autopilot/settings/target-altitude-ft</node>
			</chunk>
			<chunk>
				<name>apc</name>
				<type>double</type>
				<node>/autopilot/settings/vertical-speed-fpm</node>
			</chunk>
			<chunk>
				<name>ats</name>
				<type>double</type>
				<node>/autopilot/settings/target-speed-kt</node>
			</chunk>
			<chunk>
				<name>aph</name>
				<type>double</type>
				<node>/autopilot/settings/heading-bug-deg</node>
			</chunk>
			<chunk>
				<name>fdp</name>
				<type>double</type>
				<node>/autopilot/internal/target-pitch-deg</node>
			</chunk>
			<chunk>
				<name>fdr</name>
				<type>double</type>
				<node>/autopilot/internal/target-roll-deg</node>
			</chunk>
			<!-- Speed -->
			<chunk>
				<name>ias</name>
				<type>double</type>
				<node>/instrumentation/airspeed-indicator/indicated-speed-kt</node>
			</chunk>
			<chunk>
				<name>tas</name>
				<type>double</type>
				<node>/instrumentation/airspeed-indicator/true-speed-kt</node>
			</chunk>
			<chunk>
				<name>gs</name>
				<type>double</type>
				<node>/velocities/groundspeed-kt</node>
			</chunk>
			<chunk>
				<name>ma</name>
				<type>double</type>
				<node>/instrumentation/airspeed-indicator/indicated-mach</node>
			</chunk>
			<chunk>
				<name>iasl</name>
				<type>double</type>
				<node>/autopilot/internal/lookahead-10-sec-airspeed-kt</node>
			</chunk>
			<chunk>
				<name>iasma</name>
				<type>double</type>
				<node>/instrumentation/afds/max-airspeed-kts</node>
			</chunk>
			<chunk>
				<name>iasmi</name>
				<type>double</type>
				<node>/instrumentation/weu/state/stall-speed</node>
			</chunk>
			<!-- Altitude -->
			<chunk>
				<name>al</name>
				<type>double</type>
				<node>/instrumentation/altimeter/indicated-altitude-ft</node>
			</chunk>
			<chunk>
				<name>alr</name>
				<type>double</type>
				<node>/position/gear-agl-ft</node>
			</chunk>
			<chunk>
				<name>als</name>
				<type>double</type>
				<node>/instrumentation/altimeter/setting-inhg</node>
			</chunk>
			<chunk>
				<name>cbr</name>
				<type>double</type>
				<node>/instrumentation/vertical-speed-indicator/indicated-speed-fpm</node>
			</chunk>
			<!-- Position -->
			<chunk>
				<name>lt</name>
				<type>double</type>
				<node>/position/latitude-deg</node>
			</chunk>
			<chunk>
				<name>ln</name>
				<type>double</type>
				<node>/position/longitude-deg</node>
			</chunk>
			<chunk>
				<name>slr</name>
				<type>double</type>
				<node>/position/sea-level-radius-ft</node>
			</chunk>
			<!-- Orientation -->
			<chunk>
				<name>p</name>
				<type>double</type>
				<node>/orientation/pitch-deg</node>
			</chunk>
			<chunk>
				<name>r</name>
				<type>double</type>
				<node>/orientation/roll-deg</node>
			</chunk>
			<chunk>
				<name>h</name>
				<type>double</type>
				<node>/orientation/heading-magnetic-deg</node>
			</chunk>
			<chunk>
				<name>th</name>
				<type>double</type>
				<node>/orientation/heading-deg</node>
			</chunk>
			<!-- Flight path -->
			<chunk>
				<name>fpa</name>
				<type>double</type>
				<node>/orientation/alpha-deg</node>
			</chunk>
			<chunk>
				<name>fpb</name>
				<type>double</type>
				<node>/orientation/side-slip-deg</node>
			</chunk>
			<chunk>
				<name>tr</name>
				<type>double</type>
				<node>/orientation/track-magnetic-deg</node>
			</chunk>
			<!-- Navigation -->
			<chunk>
				<name>nav</name>
				<type>bool</type>
				<node>/instrumentation/nav/data-is-valid</node>
			</chunk>
			<chunk>
				<name>ngso</name>
				<type>bool</type>
				<node>/instrumentation/nav/gs-in-range</node>
			</chunk>
			<chunk>
				<name>ngs</name>
				<type>double</type>
				<node>/instrumentation/nav/gs-needle-deflection-norm</node>
			</chunk>
			<chunk>
				<name>nhdo</name>
				<type>bool</type>
				<node>/instrumentation/nav/in-range</node>
			</chunk>
			<chunk>
				<name>nhd</name>
				<type>double</type>
				<node>/instrumentation/nav/heading-needle-deflection-norm</node>
			</chunk>
			<chunk>
				<name>dok</name>
				<type>bool</type>
				<node>/instrumentation/dme/in-range</node>
			</chunk>
			<chunk>
				<name>dme</name>
				<type>double</type>
				<node>/instrumentation/dme/indicated-distance-nm</node>
			</chunk>
			<!-- Other -->
			<chunk>
				<name>ss</name>
				<type>double</type>
				<node>/instrumentation/slip-skid-ball/indicated-slip-skid</node>
			</chunk>
			<!-- EICAS -->
			<chunk>
				<name>thr</name>
				<type>double</type>
				<node>/controls/engines/engine/throttle</node>
			</chunk>
			<chunk>
				<name>epr</name>
				<type>double</type>
				<node>/engines/engine/epr</node>
			</chunk>
			<chunk>
				<name>n1</name>
				<type>double</type>
				<node>/engines/engine/n1</node>
			</chunk>
			<chunk>
				<name>n2</name>
				<type>double</type>
				<node>/engines/engine/n2</node>
			</chunk>
			<chunk>
				<name>egt</name>
				<type>double</type>
				<node>/engines/engine/egt-degf</node>
			</chunk>
			<!-- Wind -->
			<chunk>
				<name>wfh</name>
				<type>double</type>
				<node>/environment/wind-from-heading-deg</node>
			</chunk>
			<chunk>
				<name>ws</name>
				<type>double</type>
				<node>/environment/wind-speed-kt</node>
			</chunk>
		</output>
	</generic>
</PropertyList>
