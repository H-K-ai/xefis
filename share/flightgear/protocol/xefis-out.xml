<?xml version="1.0"?>
<PropertyList>
	<generic>
		<comment>
			Copy this file to the 'Protocol' directory of the FlightGear (usually /usr/share/flightgear/data/Protocol/).
			Run FlightGear with '--generic=socket,out,30,127.0.0.1,9000,udp,xefis-out' on command line.
			Configure the "io/flightgear" module in the Xefis configuration files. Use the same UDP port addresses.
		</comment>
		<output>
			<binary_mode>true</binary_mode>
			<byte_order>host</byte_order>
			<!-- Autopilot/autothrottle -->
			<chunk>
				<name>apa</name>
				<type>double</type>
				<node>/autopilot/settings/target-altitude-ft</node>
			</chunk>
			<chunk>
				<name>apc</name>
				<type>double</type>
				<node>/autopilot/settings/vertical-speed-fpm</node>
			</chunk>
			<chunk>
				<name>ats</name>
				<type>double</type>
				<node>/autopilot/settings/target-speed-kt</node>
			</chunk>
			<chunk>
				<name>aph</name>
				<type>double</type>
				<node>/autopilot/settings/heading-bug-deg</node>
			</chunk>
			<chunk>
				<name>fdp</name>
				<type>double</type>
				<node>/autopilot/internal/target-pitch-deg</node>
			</chunk>
			<chunk>
				<name>fdr</name>
				<type>double</type>
				<node>/autopilot/internal/target-roll-deg</node>
			</chunk>
			<!-- Speed -->
			<chunk>
				<name>ias</name>
				<type>double</type>
				<node>/instrumentation/airspeed-indicator/indicated-speed-kt</node>
			</chunk>
			<chunk>
				<name>tas</name>
				<type>double</type>
				<node>/instrumentation/airspeed-indicator/true-speed-kt</node>
			</chunk>
			<chunk>
				<name>gs</name>
				<type>double</type>
				<node>/velocities/groundspeed-kt</node>
			</chunk>
			<chunk>
				<name>ma</name>
				<type>double</type>
				<node>/instrumentation/airspeed-indicator/indicated-mach</node>
			</chunk>
			<chunk>
				<name>iasl</name>
				<type>double</type>
				<node>/autopilot/internal/lookahead-10-sec-airspeed-kt</node>
			</chunk>
			<chunk>
				<name>iasma</name>
				<type>double</type>
				<node>/instrumentation/afds/max-airspeed-kts</node>
			</chunk>
			<chunk>
				<name>iasmi</name>
				<type>double</type>
				<node>/instrumentation/weu/state/stall-speed</node>
			</chunk>
			<!-- Altitude -->
			<chunk>
				<name>std</name>
				<type>bool</type>
				<node>/instrumentation/inputs/setting-std</node>
			</chunk>
			<chunk>
				<name>al</name>
				<type>double</type>
				<node>/instrumentation/altimeter/indicated-altitude-ft</node>
			</chunk>
			<chunk>
				<name>alr</name>
				<type>double</type>
				<node>/instrumentation/altimeter/indicated-altitude-ft</node>
				<!--node>/position/gear-agl-ft</node-->
			</chunk>
			<chunk>
				<name>als</name>
				<type>double</type>
				<node>/environment/pressure-inhg</node>
			</chunk>
			<chunk>
				<name>cbr</name>
				<type>double</type>
				<node>/instrumentation/vertical-speed-indicator/indicated-speed-fpm</node>
			</chunk>
			<!-- Position -->
			<chunk>
				<name>lt</name>
				<type>double</type>
				<node>/position/latitude-deg</node>
			</chunk>
			<chunk>
				<name>ln</name>
				<type>double</type>
				<node>/position/longitude-deg</node>
			</chunk>
			<chunk>
				<name>alg</name>
				<type>double</type>
				<node>/position/altitude-ft</node>
			</chunk>
			<!-- Orientation -->
			<chunk>
				<name>p</name>
				<type>double</type>
				<node>/orientation/pitch-deg</node>
			</chunk>
			<chunk>
				<name>r</name>
				<type>double</type>
				<node>/orientation/roll-deg</node>
			</chunk>
			<chunk>
				<name>h</name>
				<type>double</type>
				<node>/orientation/heading-magnetic-deg</node>
			</chunk>
			<chunk>
				<name>th</name>
				<type>double</type>
				<node>/orientation/heading-deg</node>
			</chunk>
			<!-- Flight path -->
			<chunk>
				<name>fpa</name>
				<type>double</type>
				<node>/orientation/alpha-deg</node>
			</chunk>
			<chunk>
				<name>fpb</name>
				<type>double</type>
				<node>/orientation/side-slip-deg</node>
			</chunk>
			<chunk>
				<name>tr</name>
				<type>double</type>
				<node>/orientation/track-magnetic-deg</node>
			</chunk>
			<!-- Navigation -->
			<chunk>
				<name>nav</name>
				<type>bool</type>
				<node>/instrumentation/nav/data-is-valid</node>
			</chunk>
			<chunk>
				<name>ngso</name>
				<type>bool</type>
				<node>/instrumentation/nav/gs-in-range</node>
			</chunk>
			<chunk>
				<name>ngs</name>
				<type>double</type>
				<node>/instrumentation/nav/gs-needle-deflection-norm</node>
			</chunk>
			<chunk>
				<name>nhdo</name>
				<type>bool</type>
				<node>/instrumentation/nav/in-range</node>
			</chunk>
			<chunk>
				<name>nhd</name>
				<type>double</type>
				<node>/instrumentation/nav/heading-needle-deflection-norm</node>
			</chunk>
			<chunk>
				<name>dok</name>
				<type>bool</type>
				<node>/instrumentation/dme/in-range</node>
			</chunk>
			<chunk>
				<name>dme</name>
				<type>double</type>
				<node>/instrumentation/dme/indicated-distance-nm</node>
			</chunk>
			<!-- Other -->
			<chunk>
				<name>ss</name>
				<type>double</type>
				<node>/instrumentation/slip-skid-ball/indicated-slip-skid</node>
			</chunk>
			<chunk>
				<name>tmp</name>
				<type>double</type>
				<node>/environment/temperature-degc</node>
			</chunk>
			<!-- EICAS -->
			<chunk>
				<name>thr</name>
				<type>double</type>
				<node>/controls/engines/engine/throttle</node>
			</chunk>
			<!-- Engine 1 -->
			<chunk>
				<name>thrust1</name>
				<type>double</type>
				<node>/engines/engine/thrust_lb</node>
			</chunk>
			<chunk>
				<name>rpm1</name>
				<type>double</type>
				<node>/engines/engine/rpm</node>
			</chunk>
			<chunk>
				<name>pitch1</name>
				<type>double</type>
				<node>/engines/engine/thruster/pitch</node>
			</chunk>
			<chunk>
				<name>epr1</name>
				<type>double</type>
				<node>/engines/engine/epr</node>
			</chunk>
			<chunk>
				<name>n1-1</name>
				<type>double</type>
				<node>/engines/engine/n1</node>
			</chunk>
			<chunk>
				<name>n2-1</name>
				<type>double</type>
				<node>/engines/engine/n2</node>
			</chunk>
			<chunk>
				<name>egt1</name>
				<type>double</type>
				<node>/engines/engine/egt-degf</node>
			</chunk>
			<!-- Engine 2 -->
			<chunk>
				<name>thrust2</name>
				<type>double</type>
				<node>/engines/engine[1]/thrust_lb</node>
			</chunk>
			<chunk>
				<name>rpm2</name>
				<type>double</type>
				<node>/engines/engine[1]/rpm</node>
			</chunk>
			<chunk>
				<name>pitch2</name>
				<type>double</type>
				<node>/engines/engine[1]/thruster/pitch</node>
			</chunk>
			<chunk>
				<name>epr2</name>
				<type>double</type>
				<node>/engines/engine[1]/epr</node>
			</chunk>
			<chunk>
				<name>n1-2</name>
				<type>double</type>
				<node>/engines/engine[1]/n1</node>
			</chunk>
			<chunk>
				<name>n2-2</name>
				<type>double</type>
				<node>/engines/engine[1]/n2</node>
			</chunk>
			<chunk>
				<name>egt2</name>
				<type>double</type>
				<node>/engines/engine[1]/egt-degf</node>
			</chunk>
			<!-- Wind -->
			<chunk>
				<name>wfh</name>
				<type>double</type>
				<node>/environment/wind-from-heading-deg</node>
			</chunk>
			<chunk>
				<name>ws</name>
				<type>double</type>
				<node>/environment/wind-speed-kt</node>
			</chunk>
		</output>
	</generic>
</PropertyList>
