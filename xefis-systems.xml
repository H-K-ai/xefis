<?xml version='1.0'?>
<xefis-include>
	<modules>
		<module disabled='true' name='generic/fps'/>
		<module name='private/state'/>
		<module name='systems/lookahead'>
			<input>/systems/fms/altitude/amsl</input>
			<output>/systems/fms/altitude/amsl-lookahead</output>
			<smoothing>500</smoothing>
			<minimum-integration-time>0.03</minimum-integration-time>
			<properties>
				<property name='lookahead-time' path='/systems/fms/settings/altitude/amsl-lookahead-duration' default='10 s'/>
			</properties>
		</module>
		<module name='systems/mouse'>
			<dead-zone-x>0.05</dead-zone-x>
			<dead-zone-y>0.05</dead-zone-y>
			<acceleration-x>2.0</acceleration-x>
			<acceleration-y>2.0</acceleration-y>
			<speed-x>1.0</speed-x>
			<speed-y>1.0</speed-y>
			<properties>
				<property name='axis-x' path='/input/main/axis/9'/>
				<property name='axis-y' path='/input/main/axis/10'/>
				<property name='button' path='/input/main/button/15'/>
			</properties>
		</module>
		<module name='systems/flight-director'>
			<properties>
				<directory path='/systems/flight-director/settings'>
					<property name='enabled' path='/enabled' default='false'/>
					<property name='lateral-mode' path='/lateral-mode' default='0'/>
					<property name='vertical-mode' path='/vertical-mode' default='0'/>
					<property name='selected-magnetic-heading' path='/magnetic-heading-and-track' default='0 deg'/>
					<property name='selected-magnetic-track' path='/magnetic-heading-and-track' default='0 deg'/>
					<property name='selected-altitude' path='/altitude' default='0 ft'/>
					<property name='selected-vertical-speed' path='/vertical-speed' default='0 fpm'/>
					<property name='selected-fpa' path='/fpa' default='-3 deg'/>
				</directory>
				<directory path='/systems/flight-director/configuration'>
					<property name='pitch-limit' path='/pitch-limit' default='7 deg'/>
					<property name='roll-limit' path='/roll-limit' default='25 deg'/>
				</directory>
				<directory path='/'>
					<property name='measured-magnetic-heading' path='/sensors/compass/magnetic-heading'/>
					<property name='measured-magnetic-track' path='/systems/fms/flight-path/magnetic-track'/>
					<property name='measured-altitude' path='/systems/fms/altitude/amsl'/>
					<property name='measured-vertical-speed' path='/systems/fms/altitude/vertical-speed'/>
					<property name='measured-fpa' path='/systems/fms/flight-path/alpha'/>
					<property name='measured-pitch' path='/sensors/imu/pitch'/>
					<property name='measured-roll' path='/sensors/imu/roll'/>
				</directory>
				<directory path='/systems/flight-director'>
					<property name='output-pitch' path='/pitch'/>
					<property name='output-roll' path='/roll'/>
					<property name='vertical-mode-hint' path='/hints/vertical-hint'/>
					<property name='lateral-mode-hint' path='/hints/lateral-hint'/>
				</directory>
			</properties>
		</module>
		<module name='systems/fly-by-wire'>
			<properties>
				<directory path='/systems/fly-by-wire/config'>
					<property name='mode' path='/mode' default='0'/>
					<property name='stabilization-gain' path='/gain' default='1.0'/>
					<property name='pitch-gain' path='/pitch/gain' default='1.0'/>
					<property name='pitch-p' path='/pitch/p' default='10.0'/>
					<property name='pitch-i' path='/pitch/i' default='1.5'/>
					<property name='pitch-d' path='/pitch/d' default='0.5'/>
					<property name='pitch-error-power' path='/pitch/error-power' default='1.0'/>
					<property name='roll-gain' path='/roll/gain' default='1.0'/>
					<property name='roll-p' path='/roll/p' default='10.0'/>
					<property name='roll-i' path='/roll/i' default='1.5'/>
					<property name='roll-d' path='/roll/d' default='0.5'/>
					<property name='roll-error-power' path='/roll/error-power' default='1.0'/>
					<property name='yaw-gain' path='/yaw/gain' default='0.1'/>
					<property name='yaw-p' path='/yaw/p' default='0.1'/>
					<property name='yaw-i' path='/yaw/i' default='0.05'/>
					<property name='yaw-d' path='/yaw/d' default='0.01'/>
					<property name='yaw-error-power' path='/yaw/error-power' default='0.5'/>
					<property name='elevator-minimum' path='/elevator/minimum' default='-0.75'/>
					<property name='elevator-maximum' path='/elevator/maximum' default='+0.75'/>
					<property name='ailerons-minimum' path='/ailerons/minimum' default='-0.5'/>
					<property name='ailerons-maximum' path='/ailerons/maximum' default='+0.5'/>
					<property name='rudder-minimum' path='/rudder/minimum' default='-1.0'/>
					<property name='rudder-maximum' path='/rudder/maximum' default='+1.0'/>
					<property name='pitch-extent' path='/pitch-extent' default='20 deg'/>
					<property name='roll-extent' path='/roll-extent' default='25 deg'/>
				</directory>
				<directory path='/input'>
					<property name='input-pitch-axis' path='//input/main/axis/1'/>
					<property name='input-roll-axis' path='//input/main/axis/0'/>
					<property name='input-yaw-axis' path='//input/rudder/axis/2'/>
				</directory>
				<directory path='/systems/flight-director'>
					<property name='input-pitch' path='/pitch'/>
					<property name='input-roll' path='/roll'/>
				</directory>
				<directory path='/sensors'>
					<property name='measured-pitch' path='/imu/pitch'/>
					<property name='measured-roll' path='/imu/roll'/>
					<property name='measured-slip-skid' path='/slip-skid/slip-skid'/>
				</directory>
				<directory path='/systems/fly-by-wire'>
					<property name='output-pitch' path='/pitch'/>
					<property name='output-roll' path='/roll'/>
					<property name='output-control-stick-pitch' path='/control-stick/pitch'/>
					<property name='output-control-stick-roll' path='/control-stick/roll'/>
					<property name='output-elevator' path='/elevator'/>
					<property name='output-ailerons' path='/ailerons'/>
					<property name='output-rudder' path='/rudder'/>
				</directory>
			</properties>
		</module>
		<module name='systems/fms'>
			<properties path='/systems/fms'>
				<directory path='/settings'>
					<property name='settings.default-airplane-weight' path='/default-airplane-weight.kg' default='2.0'/>
					<property name='settings.actual-airplane-weight' path='/actual-airplane-weight.kg' default='2.0'/>
					<property name='settings.low-speed-roll-angle' path='/low-speed-roll-angle' default='15 deg'/>
					<property name='settings.speed.v-a-default' path='/speeds/v-a-default' default='90 kt'/>
					<property name='settings.speed.v-ne' path='/speeds/v-ne' default='140 kt'/>
					<property name='settings.speed.v-s' path='/speeds/v-s' default='60 kt'/>
					<property name='settings.speed.v-s0' path='/speeds/v-s0' default='53 kt'/>
					<property name='settings.speed.v-at' path='/speeds/v-at' default='69 kt'/>
					<property name='settings.speed.v-fe' path='/speeds/v-fe' default='90 kt'/>
					<property name='settings.speed.v-le' path='/speeds/v-le' default='80 kt'/>
					<property name='settings.speed.v-o' path='/speeds/v-o' default='130 kt'/>
					<property name='settings.speed.v-be' path='/speeds/v-be' default='65 kt'/>
					<property name='settings.speed.v-br' path='/speeds/v-br' default='75 kt'/>
					<property name='settings.speed.v-bg' path='/speeds/v-bg' default='65 kt'/>
					<property name='settings.flaps-configuration-properties-path' path='/speeds/flaps-configuration-properties-path' default=''/>
					<property name='settings.use-standard-pressure' path='/use-standard-pressure' default='false'/>
					<property name='settings.pressure.qnh' path='/pressure/qnh' default='29.52 inhg'/>
					<property name='settings.critical-aoa' path='/aoa/critical' default='15 deg'/>
				</directory>
				<directory path='//sensors'>
					<property name='imu.pitch' path='/imu/pitch'/>
					<property name='imu.roll' path='/imu/roll'/>
					<property name='imu.magnetic-heading' path='/compass/magnetic-heading'/>
					<property name='imu.magnetic-heading-accuracy' path='/compass/magnetic-heading-accuracy' default='5 deg'/>
					<property name='gps.longitude' path='/gps/longitude'/>
					<property name='gps.latitude' path='/gps/latitude'/>
					<property name='gps.altitude-amsl' path='/gps/altitude-amsl'/>
					<property name='gps.accuracy' path='/gps/accuracy' default='0.001 nm'/>
					<property name='gps.timestamp' path='/gps/update-timestamp'/>
					<property name='ins.longitude' path='/ins/'/>
					<property name='ins.latitude' path='/ins/'/>
					<property name='ins.altitude-amsl' path='/ins/'/>
					<property name='ins.accuracy' path='/ins/'/>
					<property name='ins.timestamp' path='/ins/'/>
					<property name='pressure.static' path='/pressure/pressure'/>
					<property name='ias' path='/speed/ias'/>
					<property name='backup-amsl' path='/backup-amsl' default='0 ft'/>
					<property name='gear-down' path='/gear-down' default='false'/>
					<property name='outside-air-temperature' path='/temperature/outside.k'/>
				</directory>
				<directory path='/position'>
					<property name='position.longitude' path='/longitude'/>
					<property name='position.latitude' path='/latitude'/>
					<property name='position.altitude-amsl' path='/altitude-amsl'/>
					<property name='position.accuracy' path='/accuracy'/>
					<property name='position.source' path='/source'/>
				</directory>
				<directory path='/track'>
					<property name='track.vertical' path='/vertical'/>
					<property name='track.true-heading' path='/true-heading'/>
					<property name='track.magnetic-heading' path='/magnetic-heading'/>
					<property name='track.vertical-delta' path='/vertical-delta'/>
					<property name='track.heading-delta' path='/heading-delta'/>
				</directory>
				<directory path='/orientation'>
					<property name='orientation.pitch' path='/pitch'/>
					<property name='orientation.roll' path='/roll'/>
					<property name='orientation.true-heading' path='/true-heading'/>
					<property name='orientation.magnetic-heading' path='/magnetic-heading'/>
				</directory>
				<directory path='/pressure-altitude'>
					<property name='pressure-altitude.amsl' path='/amsl'/>
					<property name='pressure-altitude.amsl-lookahead' path='/amsl-lookahead'/>
					<property name='pressure-altitude.amsl-lookahead-time' path='/amsl-lookahead-time'/>
					<property name='pressure-altitude.climb-rate' path='/climb-rate'/>
				</directory>
				<directory path='/speed'>
					<property name='speed.v-a' path='/v-a'/>
					<property name='speed.minimum-ias' path='/ias-minimum'/>
					<property name='speed.minimum-maneuver-ias' path='/ias-minimum-maneuver'/>
					<property name='speed.maximum-ias' path='/ias-maximum'/>
					<property name='speed.maximum-maneuver-ias' path='/ias-maximum-maneuver'/>
					<property name='speed.ias-lookahead' path='/ias-lookahead'/>
					<property name='speed.ias-lookahead-time' path='/ias-lookahead-time'/>
					<property name='speed.true-airspeed' path='/true-airspeed'/>
					<property name='speed.ground-speed' path='/ground-speed'/>
					<property name='speed.mach' path='/mach'/>
					<property name='speed.sound' path='/sound'/>
				</directory>
				<directory path='/aoa'>
					<property name='aoa.pitch-limit' path='/pitch-limit'/>
					<property name='aoa.alpha' path='/alpha'/>
					<property name='aoa.beta' path='/beta'/>
				</directory>
				<directory path='/wind'>
					<property name='wind.true-heading' path='/true-orientation-from'/>
					<property name='wind.magnetic-heading' path='/magnetic-orientation-from'/>
					<property name='wind.true-airspeed' path='/true-airspeed'/>
				</directory>
				<directory path='/localizer'>
					<property name='localizer.vertical-deviation' path='/vertical-deviation'/>
					<property name='localizer.lateral-deviation' path='/lateral-deviation'/>
					<property name='localizer.identifier' path='/identifier'/>
					<property name='localizer.source' path='/source'/>
					<property name='localizer.distance' path='/distance'/>
					<property name='localizer.frequency' path='/frequency'/>
				</directory>
				<directory path='/performance'>
					<property name='performance.climb-glide-ratio' path='/climb-glide-ratio'/>
					<property name='density-altitude' path='/density-altitude'/>
				</directory>
				<directory path='/magnetic'>
					<property name='magnetic-declination' path='/magnetic-declination'/>
					<property name='magnetic-inclination' path='/magnetic-inclination'/>
				</directory>
			</properties>
		</module>
	</modules>
</xefis-include>

